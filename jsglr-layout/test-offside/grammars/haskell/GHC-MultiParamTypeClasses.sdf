module GHC-MultiParamTypeClasses
imports Haskell-Identifier-Sorts
imports Haskell-Type-Classes

exports

	context-free syntax
		 "class" SContext? Tycls ClassParameter MaybeWhere 		-> Topdecl {cons("MPClass")}
		 
		 AType AType+ -> ClassParameter {cons("MultiParam")}
		 
		 (("(" Qtycls AType+ ")")) 					-> ClassParam
		 Qtycls ClassParam ClassParam+ 				-> Class {cons("MultiParamClass")}
		 Qtycls Tyvar Tyvar+ 						-> SimpleClass {cons("MultiParamSimpleClass"),longest-match}
	