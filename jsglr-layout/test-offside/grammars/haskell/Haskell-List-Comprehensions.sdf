module Haskell-List-Comprehensions

imports
	Haskell-Patterns
	
exports
  context-free syntax
    "[" List "]" 	 -> Exp {cons("List")}
    Exp 		 -> List
    Lexps 		 -> List
    Exp ".." 		 -> List
    Exp "," Exp ".." 	 -> List
    Exp ".." Exp 	 -> List
    Exp "," Exp ".." Exp -> List
   
    Lexps "," Exp 	 -> Lexps
    Exp "," Exp   	 -> Lexps
    
  context-free syntax
  	Exp GuardSymbol Quals -> List
     
