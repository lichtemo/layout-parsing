module GHC-BangPatterns

imports Haskell-Patterns

exports
	
	context-free syntax
		Pat	-> BPat
		APat	-> BPat {reject}
	
		"!" APat 	-> ABPat	{cons("BangPattern")}
		ABPat	-> APat
	
		ABPat Qconop LPat -> Pat {reject}
		 
	context-free priorities
		 Pat Qconop LPat -> Pat 
		 > {
		 	"!" BPat 	-> Pat {cons("BangPattern")}
		 }