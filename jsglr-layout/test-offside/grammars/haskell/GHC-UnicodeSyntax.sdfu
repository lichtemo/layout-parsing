module GHC-UnicodeSyntax

imports Haskell-Operators

exports

	lexical syntax
		"∷"		-> ProportionOp
		"←"		-> LeftArrowOp
		"→"		-> RightArrowOp
		"⇒"		-> RightDoubleArrowOp
		"⋯"		-> DoubleDotOp
		"⤙"		-> LeftArrowTailOp
		"⤚"		-> RightArrowTailOp
		"⤛"		-> DoubleLeftArrowTailOp
		"⤜"		-> DoubleRightArrowTailOp
		"★"		-> BlackStar
		"∀"		-> ForallKeyword
		
		ForallKeyword	-> ReservedOp