module GHC-ExistentialQuantification
	
	imports Haskell-Syntax
	imports GHC-ExplicitForAll
	
	exports
		
		context-free syntax
			Context Constr -> Constr {cons("ExistentialConstrs")}
			Forall Context? Constr -> Constr {cons("ExistentialConstrs")}
			
		context-free priorities
			Forall Context? Constr -> Constr
			> Context Constr -> Constr
			