module GHC-ExplicitForAll

exports

	context-free syntax
		Vars "::" "forall" Var+ "." Context? Type  -> Signdecl {cons("ExplicitForAllSignDecl")}
		
	context-free priorities
		Qvar "@" Exp -> Exp {cons("Named")}
  	  > Exp "::" "forall" Var+ "." Context? Type  -> Exp {cons("ExplicitForAllTyped")}

	lexical syntax
		"forall" -> ReservedId0
		
	lexical restrictions
		"forall" -/- [a-zA-Z0-9\'\_]