module GHC-KindSignature

imports Haskell-Syntax

exports
	
	context-free syntax
		
		"*"		 		-> AKind {cons("Kind*")}
		"!"				-> AKind {cons("Kind!")}
		"(" Kind ")"	-> AKind {cons("AKind")}
		AKind			-> Kind
		AKind "->" Kind	-> Kind {cons("Kind->")}
		
		"::" Kind	-> OptKindSig {cons("KindSignature")}
					-> OptKindSig {cons("EmptyKindSignature")}
		"(" CType "::" Kind ")" -> AType