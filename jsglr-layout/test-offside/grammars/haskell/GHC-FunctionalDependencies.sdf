module GHC-FunctionalDependencies

imports Haskell-Identifier-Sorts
imports Haskell-Type-Classes
imports Haskell-Operators

exports
	
	context-free syntax
		
		"class" SContext? Tycls AType FunctionalDependencies MaybeWhere 		-> Topdecl {cons("FDClass")}
		    
	
		Tyvar* RightArrowOp Tyvar* 					-> FunctionalDependency {ast("FunctionalDependency(<1>,<3>)")}
		"|" {FunctionalDependency ","}+ 	-> FunctionalDependencies {cons("FunctionalDependencies")}