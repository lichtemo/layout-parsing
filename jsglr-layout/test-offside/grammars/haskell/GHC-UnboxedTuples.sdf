module GHC-UnboxedTuples

imports
	Haskell-Types
	Haskell-Expressions
	Haskell-Patterns
	
exports
	
		context-free syntax
			"(#" "#)" 			-> Gtycon	{cons("TUnboxedSingleton")}
			"(#" ","+ "#)"		-> Gtycon	{cons("TUnboxedList")}
			"(#" Exps2 "#)"		-> Exp 		{cons("Unboxed")}
			"(#" {Pat ","}+ "#)"	-> APat  {cons("UnboxedPattern")}
			"(#" {CType ","}+ "#)"	-> AType {cons("UnboxedType")}