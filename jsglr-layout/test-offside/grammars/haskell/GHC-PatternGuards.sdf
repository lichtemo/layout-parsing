module GHC-PatternGuards

imports 
	Haskell-Patterns
	Haskell-Expressions

exports
	
	context-free syntax
		
		GuardSymbol {Guard ","}+			-> Guards {ast("Guards(<2>)")}
		
	context-free syntax
		
    	Pat LeftArrowOp Exp 				-> Guard {ast("PatternGuard(<1>,<3>)")}  
    	"let" Declbinds 			-> Guard {cons("LocalDeclaration")}
    	Exp							-> Guard {cons("ExpGuard")}
    	
   	 %%   "|" Exp						-> Guards {reject}
   	 