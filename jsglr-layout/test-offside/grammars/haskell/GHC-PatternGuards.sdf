module GHC-PatternGuards

imports 
	Haskell-Patterns
	Haskell-Expressions

exports
	
	context-free syntax
		
		GuardSymbol {Guard ","}+			-> Guards {cons("Guards")}
		
	context-free syntax
		
    	Pat LeftArrowOp Exp 				-> Guard {cons("PatternGuard")}  
    	"let" Declbinds 			-> Guard {cons("LocalDeclaration")}
    	Exp							-> Guard {cons("ExpGuard")}
    	
   	 %%   "|" Exp						-> Guards {reject}
   	 